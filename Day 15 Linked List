

    public static  Node insert(Node head,int data) {
        //Complete this method
           if (head == null)
            head= new Node(data);
        else {
            Node n= new Node(data);
            Node t= head;
            while(t.next != null)
                t=t.next;
            t.next=n;
        }
        return head;
    

        
    }

